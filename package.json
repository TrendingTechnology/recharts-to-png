{
  "name": "recharts-to-png",
  "version": "0.8.0",
  "description": "This package converts a Recharts chart to a png.",
  "author": "Brandon M. Mitchell",
  "license": "MIT",
  "homepage": "https://github.com/brammitch/recharts-to-png#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/brammitch/recharts-to-png"
  },
  "bugs": {
    "url": "https://github.com/brammitch/recharts-to-png/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "recharts",
    "svg",
    "download",
    "image",
    "save",
    "png",
    "react",
    "typescript"
  ],
  "scripts": {
    "tsc": "node_modules/typescript/bin/tsc && cp dist/index.d.ts index.d.ts",
    "clean": "rimraf dist",
    "build": "clean && tsc -c tsconfig.json",
    "postversion": "git push && git push --tags"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        [
          "lower-case",
          "pascal-case",
          "camel-case"
        ]
      ],
      "scope-empty": [
        2,
        "never"
      ],
      "scope-enum": [
        2,
        "always",
        [
          "lib",
          "deps",
          "release",
          "other"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "recharts": "^1.8.0"
  },
  "dependencies": {
    "html2canvas": "^1.0.0-rc.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/html2canvas": "0.0.36",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/recharts": "^1.8.18",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
